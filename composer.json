{
    "name": "crell/htmlmodel",
    "description": "Domain value objects for modeling HTML pages",
    "keywords": [
        "HTML"
    ],
    "homepage": "https://github.com/Crell/HtmlModel",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Larry Garfield",
            "email": "larry@garfieldtech.com",
            "homepage": "http://www.garfieldtech.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1.0",
        "psr/link": "^1.0",
        "fig/link-util": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Crell\\HtmlModel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crell\\HtmlModel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
